{
    "version": 2,
    "functions": {
      "app.py": {
        "runtime": "python",
        "maxDuration": 300
      }
    },
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  